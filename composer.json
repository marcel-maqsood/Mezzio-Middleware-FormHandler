{
    "name": "marcel-maqsood/mezzio-middleware-formhandler",
    "description": "The middleware formularhandler is a PSR-15 middleware that provides handling from formular data in a Laminas/Mezzio application",
    "keywords": [
        "framework",
        "MazeDEV",
        "formular",
        "middleware"
    ],
    "license": "MIT",
    "homepage": "https://github.com/marcel-maqsood/Mezzio-Middleware-FormHandler",
    "config": {
    },
    "require": {
        "php": "~8.0.0 || ~8.1.0 || ~8.2.0",
        "psr/http-server-middleware": "^1.0",
        "psr/container": "^1.0",
        "laminas/laminas-servicemanager": "^3.4",
        "swiftmailer/swiftmailer": "^6.2",
        "true/punycode": "^2.1",
        "mezzio/mezzio-problem-details": "^1.0",
        "laminas/laminas-json": "^3.1",
        "laminas/laminas-diactoros": "^2.1",
        "twig/twig": "^3.9.3"
    },
    "authors": [
        {
            "name": "Technikhafen",
            "homepage": "https://technikhafen.de"
        },
        {
            "name": "Marcel Maqsood"
        }
    ],
    "autoload": {
        "psr-4": {
            "MazeDEV\\FormularHandlerMiddleware\\": "src/"
        }
    },
    "extra": {
        "laminas": {
            "component": "MazeDEV\\FormularHandlerMiddleware",
            "config-provider": "MazeDEV\\FormularHandlerMiddleware\\ConfigProvider"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
